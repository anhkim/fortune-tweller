<div class="container">
<h3>Tweets from Users you Follow:</h3>


    <% @tweets.each do |tweet| %>
    <div class="gravatar">
      <img class="grav_small" src=" <%= tweet.user.user_gravitar_url%> " > </img>
    </div>
    <%= tweet.user.handle %>
    <%= tweet.created_at.strftime("%m/%d/%y at %I:%M%p")  %><br>
    <%= tweet.message  %><br><br>
    <% end %>


</div>